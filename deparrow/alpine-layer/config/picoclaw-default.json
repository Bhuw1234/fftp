{
  "agents": {
    "defaults": {
      "workspace": "/var/lib/deparrow/workspace",
      "model": "gpt-4o-mini",
      "max_tokens": 4096,
      "temperature": 0.7
    },
    "enabled": true,
    "auto_start": true
  },
  "deparrow": {
    "enabled": true,
    "api_url": "http://localhost:8080",
    "node_id": "",
    "auto_discover": true,
    "contribute_compute": true,
    "earn_credits": true
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18790,
    "enable_cors": true
  },
  "llm": {
    "provider": "openai",
    "api_key": "",
    "base_url": "https://api.openai.com/v1"
  },
  "memory": {
    "enabled": true,
    "path": "/var/lib/deparrow/workspace/memory",
    "max_entries": 1000,
    "ttl_hours": 168
  },
  "skills": {
    "enabled": true,
    "path": "/var/lib/deparrow/workspace/skills",
    "auto_load": true
  },
  "logging": {
    "level": "info",
    "file": "/var/log/deparrow/picoclaw.log",
    "max_size_mb": 100,
    "max_backups": 5
  }
}
